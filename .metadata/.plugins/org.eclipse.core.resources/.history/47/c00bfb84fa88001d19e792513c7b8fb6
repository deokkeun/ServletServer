package practice.run;

public class Run {

	public static void main(String[] args) {
		
		
		int answer = 0;
		int n = 123415;
        
        String str = Integer.toString(n);
        
        System.out.println(str);
        
        for(int i = 0; i < str.length(); i++) {
        	answer += Integer.parseInt(str, str.charAt(i));
        }
        
        System.out.println(answer);
	}

}
